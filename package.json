{
  "name": "vite-plugin-sveltekit-env-dynamic-public",
  "version": "1.0.0",
  "description": "Vite plugin to shim SvelteKit $env/dynamic/public for Storybook",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stayradiated/vite-plugin-sveltekit-env-dynamic-public.git"
  },
  "bugs": {
    "url": "https://github.com/stayradiated/vite-plugin-sveltekit-env-dynamic-public/issues"
  },
  "homepage": "https://github.com/stayradiated/vite-plugin-sveltekit-env-dynamic-public#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit",
    "tidy": "biome check --write --unsafe",
    "tidy:check": "biome check",
    "prepublishOnly": "$npm_execpath run tidy:check && $npm_execpath run check && $npm_execpath run build"
  },
  "peerDependencies": {
    "vite": ">=4 <8"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vite": "7.3.0"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "sveltekit",
    "svelte-kit",
    "storybook",
    "env",
    "svelte"
  ],
  "engines": {
    "node": ">=18"
  }
}
